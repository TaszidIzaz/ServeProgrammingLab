<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Books</title>
</head>
<body>
    <table>
        <th>Name</th>
        <th>Author</th>
        <th>Genre</th>

        <% if(data.length){
 
            for(var i = 0; i< data.length; i++) {%>  
            <tr>
              <th scope="row"><%= (i+1) %></th>
              <td><%= data[i].name%></td>
              <td><%= data[i].author%></td>
              <td><%= data[i].genre%></td>
            </tr>
            <% }
                     
             }else{ %>
                 <tr>
                    <td colspan="3">No user</td>
                 </tr>
              <% } %>    
    </table>
</body>
</html>